{
  "root": true,
  "extends": ["warp/es6", "warp/jsx"],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": "off"
  },
  "env": {"browser": true},
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"]
}
